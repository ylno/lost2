rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
   
    match /cache-sessions/{sessionId}/chat/{document=**} {
      allow read: if true; // Erm√∂glicht jedem, der den Pfad kennt, den Lesezugriff
    }
    
     match /{document=**} {
      allow read, write: if false;
    }
  }
  
}